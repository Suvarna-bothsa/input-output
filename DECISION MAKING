    print("no")
    
#Budget

A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
n=(A*350.34)+(B*230.90)+(C*190.55)+(D*125.30)+(E*180.90)
if n<15000:
    print("yes")
else:
    print("no")
    
#Sece Dining

a=input()
b=int(input())
if a=='front' and b==1 or a=='rear' and b==2:
    print("left handed")
else:
    print("right handed")
    
#Intersection of a Circle

x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
r=r1+r2
d=(((x2-x1)**2)+((y2-y1)**2)**(1/2))
if d==r:
    print("the circles are tangential")
elif d<r:
    print("the circles overlap")
else:
    print("the circles do not overlap")
    
#Microwave Oven

print("enter the number of items")
a=int(input())
print("enter the single item heating time")
b=float(input())
if a==2:
    print("the recommended heating time is ","%.2f"%(b+b*50/100))
elif a==3:
    print("the recommended heating time is ","%.2f"%(2*b))
elif a==1:
    print("the recommended heating time is ",b)
else:
    print("number of items is more")
